	<entry>
		<title>$title</title>
		<link href="$url"/>
		<id>$id</id>
		<category term="$category"/>
		<updated>$updated</updated>
		<published>$published</published>
		<author>
			<name>$author</name>
		</author>
		<summary type="html"><![CDATA[$summary]]></summary>
		<content type="html">
			<![CDATA[
				<span style="white-space:pre-wrap;">$contents</span>
			]]>
		</content>
	</entry>
